# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Processing Configuration
OPENAI_MODEL=gpt-4o
CLASSIFIER_MODEL=gpt-4o-mini
INPUT_DIR=pdfs
OUTPUT_CSV=email_data.csv
PROCESSED_LOG=processed_files.txt
SKIPPED_LOG=skipped_non_emails.txt
ERROR_LOG=error_log.txt
MAX_RETRIES=3

# Multi-Email PDF Configuration
# MAX_PAGES_PER_PDF - Maximum pages to process per PDF (leave empty for unlimited)
MAX_PAGES_PER_PDF=
# SPLIT_MULTI_EMAILS - Detect and split multi-email PDFs into separate CSV rows (true/false)
SPLIT_MULTI_EMAILS=true

